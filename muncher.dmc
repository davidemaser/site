dmc = include(dmc, {core,server});
dmc.local extends dmc as local;
dmc.each(as feature)
    loop(feature=all) =>
        return object.definition();
    exit::
end::

react = define from ['React','Component','Script'];
react.core uses react as object;

set Object.Core(dmc,dmc[null]) =>
    Core.create(Array=OBJarray)
        define Core.OBJarray(key,value) =>
            OBJarray.(key[value]) or null
        exit::
    exit::
end::

import [def] from '/usr/%usr.defines%':cache(true)